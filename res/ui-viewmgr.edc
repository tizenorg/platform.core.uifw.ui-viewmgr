               max: TIZEN_VIEW_BACK_BUTTON_TITLE_RIGHT_PADDING_WIDTH_INC -1;
               align: 1.0 0.0;
               fixed: 1 1;
               rel1 { to: "title_bg"; relative: 1.0 0.0; }
               rel2 { to: "title_bg"; relative: 1.0 1.0; }
            }
         }
         part { name: "toolbar_more_btn";
            type: SWALLOW;
            clip_to: "title_clipper";

            description { state: "default" 0.0;
               min: 0 TIZEN_VIEW_TABBAR_TITLE_CLIPPER_HEIGHT_INC;
               max: -1 TIZEN_VIEW_TABBAR_TITLE_CLIPPER_HEIGHT_INC;
               fixed: 0 1;
               align: 0.5 1.0;
               rel1 { to: "tabbar"; relative: 0.0 1.0; }
               rel2 { to: "tabbar"; relative: 1.0 1.0; }

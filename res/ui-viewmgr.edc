               rel1 { to: "tabbar_clipper"; relative: 0.0 0.0; }
               rel2 { to: "tabbar_clipper"; relative: 1.0 0.0; }
               color: 244 244 244 25;
               visible: 0;
            }
            description { state: "visible" 0.0;
               inherit: "default" 0.0;
               min: 0 TIZEN_VIEW_TABBAR_TOP_DIVIDER_HEIGHT_INC;

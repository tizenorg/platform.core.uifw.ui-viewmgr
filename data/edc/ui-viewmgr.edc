            }
            desc { "visible";
               inherit: "default";
               min: 0 TIZEN_VIEW_TOOLBAR_TITLE_HEIGHT_INC;
               max: -1 TIZEN_VIEW_TOOLBAR_TITLE_HEIGHT_INC;
               visible: 1;
            }
         }
         rect { "toolbar_top_divider";

               max: -1 TIZEN_VIEW_TOOLBAR_TITLE_TOP_PADDING_HEIGHT_INC;
               fixed: 0 1;
               align: 0.5 0.0;
               rel1 {
                  to: "title_bg";
                  relative: 0.0 0.0;
               }
               rel2 {
                  to: "title_bg";
                  relative: 1.0 0.0;
               }
            }
         }
         swallow { "toolbar";
            scale: 1;
            clip_to: "toolbar_clipper";
            desc { "default";
               min: 0 0;
               max: -1 0;
               fixed: 0 1;
               align: 0.5 0.0;
               rel1 {
                  to: "toolbar_top_padding";
                  relative: 0.0 1.0;
               }
               rel2 {
                  to: "toolbar_top_padding";
                  relative: 1.0 1.0;
               }

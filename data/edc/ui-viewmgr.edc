               max: -1 0;
               fixed: 0 1;
               align: 0.5 0.0;
               rel1 {
                  to: "toolbar_clipper";
                  relative: 0.0 0.0;
               }
               rel2 {
                  to: "toolbar_clipper";
                  relative: 1.0 0.0;

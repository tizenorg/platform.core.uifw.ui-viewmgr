            mouse_events: 0;
            clip_to: "clipper";
            desc { "default";
               rel1.to: "base";
               rel2.to: "base";
               align: 0.5 0.0;
               fixed: 0 1;
               min: TIZEN_VIEW_BG_LANDSPACPE_HEIGHT_INC TIZEN_VIEW_BG_PORTRAIT_HEIGHT_INC;
               image.normal: "images/core_theme_bg_01.png";
            }
            desc { "landscape";
               inherit: "default";
               min: 0 TIZEN_VIEW_BG_LANDSPACPE_HEIGHT_INC;
            }
         }
         rect { "clipper";
            mouse_events: 0;
            desc { "default";
               visible: 1;
            }
            desc { "hidden";
               visible: 0;
            }
         }
         rect { "title_bg";
            scale: 1;
            mouse_events: 0;
            desc { "default";
               min: 0 TIZEN_VIEW_TITLE_HEIGHT_INC;
               max: -1 TIZEN_VIEW_TITLE_HEIGHT_INC;
               align: 0.0 0.0;
               fixed: 0 1;
               rel1 {
                  to: "base";
                  relative: 0.0 0.0;
               }
               rel2 {
                  to: "base";
                  relative: 1.0 0.0;
               }
               color: 61 185 204 255;
            }
            desc { "hidden";
               inherit: "default";
               min: 0 0;
               max: -1 0;
            }
         }
